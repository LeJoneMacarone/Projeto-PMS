    <%- include("./partials/begin.ejs");  %>
    <title>Creator Campaigns to Evaluate</title>
</head>
<body>
    <%- include("./partials/nav_bar.ejs") %>
    <div>
        <%- include("./partials/creator_opt_bar.ejs") %>
        <table>
            <tr>
                <th>Title</th>
                <th>State</th>
                <th>Send Date<th>
                <th>Admin that evaluate</th>
            </tr>
            
            <% campaigns.forEach((campaign) => { %>
                <tr>
                    <td><%= campaign.title %></td>
                    <td><%= campaign.state %></td>
                    <td><%= campaign.date %></td>
                    <td><% campaign.state == "Evaluating" ? "---" : campaign.eval_admin %></td>
                </tr>
            <% }); %>
        </table>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>